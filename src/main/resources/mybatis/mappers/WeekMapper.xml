<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qgh.dao.WeekDao">

    <select id="searchAll" resultType="com.qgh.pojo.Week">
        select * from week
    </select>
    <select id="searchAllById" resultType="com.qgh.pojo.Week">
        select * from week
        by id
    </select>

</mapper>